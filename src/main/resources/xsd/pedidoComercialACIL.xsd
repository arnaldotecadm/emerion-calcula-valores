<?xml version="1.0" encoding="UTF-8"?><!-- edited with XMLSPY v2004 rel. 3 U (http://www.xmlspy.com) by Waitrose Systems (John Lewis Plc) --><xs:schema	xmlns:xs="http://www.w3.org/2001/XMLSchema"	elementFormDefault="qualified">	<xs:element name="pedidoComercialACIL">		<xs:complexType>			<xs:sequence>				<xs:element name="cabecalho">					<xs:complexType>						<xs:sequence>							<xs:element								name="userCreatingChange"								type="xs:string" />							<xs:element name="userType">								<xs:simpleType>									<xs:restriction base="xs:string">										<xs:enumeration value="admin" />										<xs:enumeration value="customer" />									</xs:restriction>								</xs:simpleType>							</xs:element>							<xs:element								name="changeDescription"								type="xs:string" />							<xs:element								name="updateTimestamp"								type="xs:dateTime" />						</xs:sequence>					</xs:complexType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element		name="quantity"		type="xs:decimal" />	<xs:element		name="areSubstitutionsAllowed"		type="xs:boolean" />	<xs:element		name="paperStatementInd"		type="xs:boolean" />	<xs:element		name="smsPrefInd"		type="xs:boolean" />	<xs:element		name="driverComment"		type="xs:string" />	<xs:element		name="driverCommentTimeStamp"		type="xs:dateTime" />	<xs:element		name="sequenceNumber"		type="xs:integer" />	<xs:element		name="isSupplierOrderRequired"		type="xs:boolean" />	<xs:element name="status">		<xs:simpleType>			<xs:restriction base="xs:string">				<xs:enumeration value="placed" />				<xs:enumeration value="cancelled" />				<xs:enumeration value="picking" />				<xs:enumeration value="in transit" />				<xs:enumeration value="completed" />			</xs:restriction>		</xs:simpleType>	</xs:element>	<xs:element name="wherePlaced">		<xs:simpleType>			<xs:restriction base="xs:string">				<xs:enumeration value="home" />				<xs:enumeration value="phone" />				<xs:enumeration value="branch" />			</xs:restriction>		</xs:simpleType>	</xs:element>	<xs:element name="type">		<xs:simpleType>			<xs:restriction base="xs:string">				<xs:enumeration value="grocery" />				<xs:enumeration value="entertaining" />				<xs:enumeration value="mixed" />				<xs:enumeration value="loan" />				<xs:enumeration value="seasonal" />				<xs:enumeration value="flowers" />				<xs:enumeration value="hampers" />				<xs:enumeration value="wines" />				<xs:enumeration value="food+" />			</xs:restriction>		</xs:simpleType>	</xs:element>	<xs:element		name="offerIdentifier"		type="xs:string" /></xs:schema>